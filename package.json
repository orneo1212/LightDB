{
  "name": "lightdb",
  "version": "0.1.0",
  "description": "Lightweight JSON document store",
  "main": "server.js",
  "scripts": {
    "test": "tape \"tests/**/*.js\" | faucet",
    "testnodemon": "nodemon -e js -x \"reset && pnpm test\"",
    "testraw": "tape \"tests/**/*.js\"",
    "buildbrowser": "browserify -e browserbundle.js -o public/lightdb.browser.js",
    "build": "pkg --debug -c package.json -o dist/lightdb-server server.js"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "db",
    "json",
    "database",
    "store"
  ],
  "author": "orneo1212",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^17.0.0",
    "faucet": "^0.0.1",
    "pkg": "^5.3.2",
    "tape": "^5.3.1"
  },
  "dependencies": {
    "audit": "^0.0.6",
    "axios": "^0.21.4",
    "fastify": "^3.21.0",
    "fastify-basic-auth": "^2.1.0",
    "fastify-etag": "^2.1.0",
    "fastify-multipart": "^5.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-static": "^4.2.3"
  },
  "pkg": {
    "targets": [
      "node14-linux-x64"
    ],
    "assets": "public/**/*",
    "scripts": [],
    "outputPath": "dist"
  }
}